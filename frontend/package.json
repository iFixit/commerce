{
   "name": "@ifixit/commerce-frontend",
   "license": "UNLICENSED",
   "version": "1.0.0",
   "description": "Next.js frontend for iFixit e-commerce",
   "author": "Massimo",
   "repository": {
      "url": "https://github.com/iFixit/react-commerce"
   },
   "scripts": {
      "dev": "concurrently -n \"Next,codegen\" -c \"blue,magenta\" \"pnpm run dev:next\" \"pnpm run dev:codegen\"",
      "dev:next": "next dev",
      "dev:codegen": "cross-env DOTENV_CONFIG_PATH=./.env.development graphql-codegen -r dotenv/config --config codegen.yml --watch",
      "codegen:download-shopify-storefront-schema": "node ./lib/shopify-storefront-sdk/generate-schema.js",
      "build": "next build",
      "start": "next start",
      "type-check": "tsc --pretty --noEmit",
      "format": "prettier --write .",
      "lint": "eslint . --ext ts --ext tsx --ext js",
      "test": "jest",
      "test-all": "pnpm run lint && pnpm run type-check && pnpm run test",
      "cypress:open": "cypress open",
      "cypress:run": "cypress run",
      "playwright:run": "playwright test",
      "transform-svg": "npx @svgr/cli --typescript -d assets/svg assets/svg/files && prettier --write assets/svg"
   },
   "dependencies": {
