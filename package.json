{
  "name": "@ifixit/commerce",
  "version": "1.0.0",
  "description": "Monorepo for iFixit e-commerce",
  "main": "N/A",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "wait-on http://localhost:1337/_health && cd frontend && npm run dev",
    "dev": "cross-env FORCE_COLOR=1 npm-run-all -l -p dev:*",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "build": "npm run build:backend && npm run build:frontend",
    "start:backend": "cd backend && npm run start",
    "start:frontend": "cd frontend && npm run start",
    "start": "cross-env FORCE_COLOR=1 npm-run-all -l -p start:*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "npm": "npm install && npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "transform-svg": "cd frontend && npm run transform-svg"
  },
  "keywords": [
    "ifixit",
    "e-commerce",
    "repair"
  ],
  "author": "macs",
  "license": "UNLICENSED",
  "devDependencies": {
    "cross-env": "7.0.3",
    "npm-run-all": "4.1.5",
    "wait-on": "6.0.0",
    "prettier": "2.3.2"
  }
}
